
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.2/css/bootstrap.css" />
<link rel="stylesheet" href="https://cdn.datatables.net/1.11.4/css/dataTables.bootstrap4.min.css" />
<script src="https://use.fontawesome.com/65d1ed3b51.js"></script>

<br />
<br />

<div class="card shadow mb-4">

  <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">
            Apprentice: Continue Application or Withdraw Application
        </h6>
  </div>

<div class="card-body">

      <div class="table table-striped table-bordered" id="dataTable" width="100%" cellspacing="0">
        <table class="table thead-dark table-sm">
  <thead>
    <tr>
      <th scope="col">Full Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">ID Number</th>
      <th scope="col">Course</th>
      <th scope="col">Ref Number</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>@ViewData["Name"]</td>
      <td>@ViewData["LName"]</td>
      <td>@ViewData["RSAID"]</td>
      <td>@ViewData["Course"]</td>
      <td>@ViewData["Ref"]</td>
 
      <td>
      <li class="btn btn-outline-success"><a href="@Url.Action("ModifyApplicant", "Applicant", new { id = @ViewData["UserId"]})"><i class="fa fa-user"></i> Continue</a></li>
      <li class="btn btn-danger"><a href="@Url.Action("ModifyApplicant", "Applicant", new { id = @ViewData["UserId"]})"><i class="fa fa-close"></i> Withdraw</a></li>

      </td>
    </tr>
  </tbody>
</table>
</div>


</div>
</div>




